{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"shell-app": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/shell-app",
			"sourceRoot": "projects/shell-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/shell-app",
						"index": "projects/shell-app/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/shell-app/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/shell-app/public",
								"output": "."
							}
						],
						"styles": [
							"projects/shell-app/src/styles.css"
						],
						"scripts": [],
						"main": "projects/shell-app/src/main.ts",
						"extraWebpackConfig": "projects/shell-app/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/shell-app/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "shell-app:build:production",
							"extraWebpackConfig": "projects/shell-app/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "shell-app:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/shell-app/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular/build:extract-i18n"
				},
				"test": {
					"builder": "@angular/build:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/shell-app/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/shell-app/public"
							}
						],
						"styles": [
							"projects/shell-app/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
		"products-app": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/products-app",
			"sourceRoot": "projects/products-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/products-app",
						"index": "projects/products-app/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/products-app/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/products-app/public",
								"output": "."
							}
						],
						"styles": [
							"projects/products-app/src/styles.css"
						],
						"scripts": [],
						"main": "projects/products-app/src/main.ts",
						"extraWebpackConfig": "projects/products-app/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/products-app/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "products-app:build:production",
							"extraWebpackConfig": "projects/products-app/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "products-app:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/products-app/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular/build:extract-i18n"
				},
				"test": {
					"builder": "@angular/build:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/products-app/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/products-app/public"
							}
						],
						"styles": [
							"projects/products-app/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"cli": {
		"analytics": "54042f00-ed68-4fc5-ab02-11bd46682ce1"
	},
	"schematics": {
		"@schematics/angular:component": {
			"type": "component"
		},
		"@schematics/angular:directive": {
			"type": "directive"
		},
		"@schematics/angular:service": {
			"type": "service"
		},
		"@schematics/angular:guard": {
			"typeSeparator": "."
		},
		"@schematics/angular:interceptor": {
			"typeSeparator": "."
		},
		"@schematics/angular:module": {
			"typeSeparator": "."
		},
		"@schematics/angular:pipe": {
			"typeSeparator": "."
		},
		"@schematics/angular:resolver": {
			"typeSeparator": "."
		}
	}
}